modules = ["flutter", "python-3.11"]
[[ports]]
localPort = 5000
externalPort = 80

[agent]
integrations = ["javascript_object_storage==1.0.0"]

[nix]
packages = ["hol", "zip"]
channel = "stable-25_05"

[deployment]
deploymentTarget = "cloudrun"
run = ["sh", "-c", "cd build/web && python3 -m http.server 5000 --bind 0.0.0.0"]
